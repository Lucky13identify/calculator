{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/$1",
      "headers": {
        "Cache-Control": "public, max-age=0, must-revalidate"
      },
      "continue": true
    }
  ],
  "env": {
    "GITHUB_TOKEN": "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"
  },
  "github": {
    "silent": false,
    "silentComment": true,
    "autoComments": true,
    "autoIssueComments": true
  },
  "alias": {
    "your-alias.vercel.app": "your-github-repo.vercel.app"
  }
}
